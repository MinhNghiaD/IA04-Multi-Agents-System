package lib;
import jade.core.Agent;

public class HelloWorld extends Agent 
{
	protected void setup() 
	{
		// Printout a welcome message
		System.out.println("Hello world!");
	}
}

public class HelloWorldBehaviour extends Behaviour 
{
	 public void action() 
	 {
		 while (true) 
		 {
			 // do something
		 }
	 }
	 
	 public boolean done() 
	 {
		 return true;
	 }
}